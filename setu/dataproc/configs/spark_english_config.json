    "non_li_filter": true,
    "non_li_char_threshold": 0.05,
    "line_count_filter": true,
    "min_line_count": 4,
    "line_length_filter": true,
    "min_mean_line_len": 5,
    "word_repetition_filter": true,
    "word_ngram_cum_thresholds": {
        "5": 0.4,
        "6": 1.1,
        "7": 1.1,
        "8": 1.1,
        "9": 1.1
    },
    "char_repetition_filter": true,
    "char_ngram_cum_thresholds": {
        "6": 1.1,
        "7": 1.1,
        "8": 1.1,
        "9": 1.1,
        "10": 0.2
    }
